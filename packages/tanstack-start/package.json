{
  "name": "@nightmar3/uauth-tanstack-start",
  "version": "1.0.1",
  "description": "TanStack Start integration for Universal Auth SDK - Zero config authentication with full SSR support",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.mjs",
      "require": "./dist/client.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.mjs",
      "require": "./dist/server.js"
    },
    "./middleware": {
      "types": "./dist/middleware.d.ts",
      "import": "./dist/middleware.mjs",
      "require": "./dist/middleware.js"
    },
    "./context": {
      "types": "./dist/context.d.ts",
      "import": "./dist/context.mjs",
      "require": "./dist/context.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup src/index.ts src/client.tsx src/server.ts src/middleware.ts src/context.tsx --format cjs,esm --dts --clean --external @tanstack/react-start --external @tanstack/react-router --external react",
    "dev": "tsup src/index.ts src/client.tsx src/server.ts src/middleware.ts src/context.tsx --format cjs,esm --watch --external @tanstack/react-start --external @tanstack/react-router --external react",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "auth",
    "authentication",
    "tanstack",
    "tanstack-start",
    "tanstack-router",
    "react",
    "ssr",
    "oauth",
    "universal-auth"
  ],
  "author": "nightmar3",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Amsiam/uauth.git",
    "directory": "packages/tanstack-start"
  },
  "peerDependencies": {
    "@tanstack/react-router": "^1.0.0",
    "@tanstack/react-start": "^1.0.0",
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@tanstack/react-router": "^1.0.0",
    "@tanstack/react-start": "^1.0.0",
    "@types/react": "^18.2.45",
    "@vitest/ui": "^1.0.4",
    "jsdom": "^23.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4"
  },
  "dependencies": {
    "@nightmar3/uauth-core": "^1.0.0",
    "@nightmar3/uauth-server": "^1.0.0"
  }
}
